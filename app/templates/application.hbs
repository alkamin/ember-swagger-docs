      <!-- New operation section-->
      <section class="operation get" id="using-the-api">
        <div class="operation-area">
          <!-- inside .operation-details is where we talk about the operation in detail -->
          <!-- This is the middle column -->
          <div class="operation-details">
            <!-- Each opertion section has a heading -->
            <!-- They all have a title -->
            <!-- Top level sections use <h1>, second level sections use an <h2> and third use <h3> -->
            <div class="heading">
              <!-- If in third level section include a link to parent section -->
              <a class="parent-tag" href="#projects">Projects</a>

              <!-- Might need to use swagger extension for this -->
              <h3>List of Projects</h3>

              <!-- The http method -->
              <!-- Remeber to include a .get, .delete, .post, etc class to class="http_method" -->
              <pre class="definition"><code><span class="http_method get">get</span><span class="path">/projects/</span></code></pre>
            </div>

            <!-- Currently this is the summary -->
            <p>Get list of projects the user is authorized to view</p>

            <!-- All parameters are wrapped in .parameters -->
            <div class="parameters">
              <h4>Parameters</h4>

              <!-- single paramater -->
              <div class="parameter">
                <div class="parameter-details">
                  <!-- paramater name -->
                  <label class="parameter-name" for="ordering">Ordering</label>
                  <!-- paramater schema type -->
                  <code class="parameter-type string">string</code>
                </div>
                <div class="parameter-input">
                  <!-- paramater schema -->
                  <select id="ordering" class="form-control">
                    <option></option>
                    <option>createdAt,desc</option>
                    <option>createdAt,asc</option>
                    <option>modifiedAt,desc</option>
                    <option>modifiedAt,asc</option>
                  </select>
                  <!-- paramater requirement -->
                  <div class="parameter-requirement">optional</div>
                </div>
                <!-- paramater Description -->
                <div class="parameter-description">Field to order results by; meaning of label varies based on tool</div>
              </div>
              <div class="parameter">
                <div class="parameter-details">
                  <label class="parameter-name" for="pageSize">pageSize</label>
                  <code class="parameter-type number">number</code>
                </div>
                <div class="parameter-input">
                  <input type="number" id="pageSize" class="form-control" min="0">
                  <div class="parameter-requirement">optional</div>
                </div>
                <div class="parameter-description">Number of results per page in paginated response</div>
              </div>
              <div class="parameter">
                <div class="parameter-details">
                  <label class="parameter-name" for="page">page</label>
                  <code class="parameter-type number">number</code>
                </div>
                <div class="parameter-input">
                  <input type="number" id="page" class="form-control" min="0">
                  <div class="parameter-requirement">optional</div>
                </div>
                <div class="parameter-description">Page of results to go to</div>
              </div>
              <div class="parameter">
                <div class="parameter-details">
                  <label class="parameter-name" for="organization">organization</label>
                  <code class="parameter-type string">string</code>
                </div>
                <div class="parameter-input">
                  <input type="text" id="organization" class="form-control" placeholder="UUID">
                  <div class="parameter-requirement">optional</div>
                </div>
                <div class="parameter-description">UUID for organization to filter by</div>
              </div>
              <div class="parameter">
                <div class="parameter-details">
                  <label class="parameter-name" for="tags">tags</label>
                  <code class="parameter-type string">string</code>
                </div>
                <div class="parameter-input">
                  <input type="text" id="organization" class="form-control" placeholder="">
                  <div class="parameter-requirement">optional</div>
                </div>
                <div class="parameter-description">Only return results that contain these tags</div>
              </div>
            </div>

            <div class="responses">
              <h4>Responses</h4>
              <div class="response">
                <div class="response-code">200</div>
                <div class="response-description">Paginated list of projects the user is authorized to view</div>
              </div>
            </div>
          </div>

          <!-- inside .operation-samples is where we show our request and response code -->
          <!-- This is the right most column -->
          <div class="operation-samples">

            <!-- Each example has its own sample-block -->
            <!-- Right now we only have request and response -->
            <div class="sample-block">
              <div class="sample-block-title">Example Request</div>
              <pre><code>curl https://api.rasterfoundry.com/projects/tr_19xi9f2eZvKYlo2CrT9LKC2r \
              -d ordering="createdAt,desc"
              </code></pre>
                          </div>
                          <div class="sample-block">
                            <div class="sample-block-title">Example Response</div>
                            <pre><code>{
              "object": "list",
              "url": "/projects/",
              "data": [
              {
                "id": "sdjkfhsfh938"
                ...
              }
              ]

              }</code></pre>
            </div>
          </div>
        </div>
      </section>

